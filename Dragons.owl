    <!-- http://www.owl-ontologies.com/Ontology1569341988.owl#MAEONIAN_DRACON_(1) -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Ontology1569341988.owl#MAEONIAN_DRACON_(1)">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Dracones_Mythical"/>
        <has_Form rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Giant_serpent"/>
        <is_Killed_By rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Heracles"/>
        <lives_In rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#River_Sagaris_in_Lydia"/>
        <has_Info rdf:datatype="http://www.w3.org/2001/XMLSchema#string">THE DRAKON MAIONIOS (Lydian Dragon) was a giant serpent which ravaged the Anatolian kingdom of Lydia (Maionia). It was slain by Herakles in the service of Queen Omphale. The hero&apos;s victory was afterwards memorialised amongst the stars as the constellation Ophiochus.</has_Info>
        <has_Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">THE LYDIAN DRAGON</has_Name>
        <has_Summary rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MAEONIAN DRACON (1) (Drakon Maionios) A dragon which ravaged the kingdom of Lydia. It was slain by Heracles when he was in the service of Queen Omphale.</has_Summary>
    </owl:NamedIndividual>
    


    <!-- http://www.owl-ontologies.com/Ontology1569341988.owl#MAEONIAN_DRACON_(2) -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Ontology1569341988.owl#MAEONIAN_DRACON_(2)">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Dracones_Mythical"/>
        <has_Form rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Giant_serpent"/>
        <is_Killed_By rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Damasen"/>
        <lives_In rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#River_Hermus_in_Lydia"/>
        <has_Info rdf:datatype="http://www.w3.org/2001/XMLSchema#string">THE DRAKON MAIONIOS (Lydian Dragon) was a giant serpent of the River Hermos in Lydia, Anatolia. It was slain by the hero Damasen but its mate used a magical herb to restore it to life.</has_Info>
        <has_Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">THE LYDIAN DRAGON</has_Name>
        <has_Summary rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MAEONIAN DRACON (2) (Drakon Maionios) A monstrous dragon which terrorized the kingdom of Lydia. It was slain by the giant Damasen.</has_Summary>
    </owl:NamedIndividual>
    


    <!-- http://www.owl-ontologies.com/Ontology1569341988.owl#MEDEA&apos;S_DRACONES -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Ontology1569341988.owl#MEDEA&apos;S_DRACONES">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Dracones_Mythical"/>
