    <!-- http://www.owl-ontologies.com/Ontology1569341988.owl#DEMETER&apos;S_DRACONES -->

    <owl:NamedIndividual rdf:about="http://www.owl-ontologies.com/Ontology1569341988.owl#DEMETER&apos;S_DRACONES">
        <rdf:type rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Dracones_Mythical"/>
        <has_Form rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#monster3"/>
        <has_Parent rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Metanira"/>
        <lives_In rdf:resource="http://www.owl-ontologies.com/Ontology1569341988.owl#Eleusis"/>
        <has_Info rdf:datatype="http://www.w3.org/2001/XMLSchema#string">TRIPTOLEMOS (Triptolemus) was a demi-god of the Eleusinian mysteries who presided over the sowing of grain-seed and the milling of wheat.</has_Info>
        <has_Name rdf:datatype="http://www.w3.org/2001/XMLSchema#string">DEMI-GOD TRIPTOLEMUS</has_Name>
        <has_Summary rdf:datatype="http://www.w3.org/2001/XMLSchema#string">DEMETER&apos;S DRACONES A pair of winged dragons which drew the chariot of goddess Demeter. She gave them to the hero Triptolemos to carry him across the world spreading knowledge of agriculture.</has_Summary>
    </owl:NamedIndividual>
    


